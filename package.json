{
    "name": "bibi-ds",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "init:storybook": "turbo run init --filter=storybook",
        "build": "turbo run build --filter=storybook",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint --parallel",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "start:ui": "turbo run start --parallel --filter=ui --include-dependencies",
        "watch:storybook": "turbo run watch --filter=storybook --include-dependencies",
        "test": "turbo run test --filter=storybook"
    },
    "devDependencies": {
        "@headlessui/react": "^1.7.8",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "eslint-config-custom": "*",
        "prettier": "latest",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "turbo": "latest",
        "typescript": "^4.7.4"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "packageManager": "npm@8.1.2",
    "peerDependencies": {
        "react": ">= 17",
        "react-dom": ">= 17"
    }
}
